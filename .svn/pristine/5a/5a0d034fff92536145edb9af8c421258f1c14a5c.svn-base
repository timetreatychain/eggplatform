body {
    position: absolute;
    width: 100%;
    height: 100%;
}
#gameIframeDiv{
    position: absolute;
}
.refreash{
    position: fixed;
    bottom: 0;
    text-align: center;
}

.controlBall {
    position: absolute;
    border-radius: 100%;
    overflow: hidden;
    width: 0.8rem;
    height: 0.8rem;
    max-height: 55px;
    max-width: 55px;
    left: 0;
    top: 45%;
    z-index: 777;
    transform: translateX(50%);
    img {
        width: 0.8rem;
        height: 0.8rem;
          max-height: 55px;
    max-width: 55px;
    }
}
.slideBox2{
    width: 15%;
    height: 100%;
    position: absolute;
    right: 0;
    top: 0;
    z-index: 888;
    background: rgba(0,0,0,0.5);
}

.pay{
    width: 6.4rem;
    height: 100%;
    background-color: #F7F7F7;
    position: fixed;
    bottom: 0;
    z-index: 999;
    .payHeader{
        height: 0.88rem;
        background-color: #ff6633;
        color: #ffffff;
        text-align: center;
        line-height: 0.88rem;
        font-size: 0.3rem;
        img{
            position: absolute;
            left: 0;
            top: 0;
            height: 0.88rem;
        }
    }
    .payMess{
        background-color: #ffffff;
        width: 100%;
        position: absolute;
        top: 1.23rem;
        height: 1.24rem;
        .goodsPrice{
            padding-top: 0.27rem;
            line-height: 0.4rem;
            font-size: 0.4rem;
            text-align: center;
            color: #ff6633;
            display: block;
        }
        .goodsName{
            display: block;
            font-size: 0.2rem;
            text-align: center;
            color: #999999;
            line-height: 0.3rem;
        }
    }
    .payWays{
        position: absolute;
        top: 2.56rem;
//      height: 1.9rem;
        background-color: #ffffff;
        width: 6.4rem;
        li{
            border-bottom: 1px solid #f7f7f7;
            height: 0.98rem;
            overflow: hidden;
            .imgL{
                width: 0.32rem;
                margin: 0.34rem 0.3rem 0.34rem 0.2rem;
                float: left;
            }
            span{
                line-height: 0.98rem;
                color: #4a494b;
            }
            .imgR{
                width: 0.38rem;
                margin: 0.3rem 0.2rem;
                float: right;
            }
        }
    }
    .payOk{
        position: absolute;
        bottom:1rem ;
        width: 5.85rem;
        margin-left: 0.28rem;
        line-height: 0.78rem;
        text-align: center;
        background-color: #ff6633;
        font-size: 0.26rem;
        color: #ffffff;
        border-radius: 0.1rem;
        
    }
    
}

.slideBox {
    z-index: 888;
    position: absolute;
    top: 0;
    background-color: #ffffff;
    width: 85%;
    height: 100%;
    .user {
        position: absolute;
        height: 1.25rem;
        border-bottom: 0.1rem solid #f7f7f7;
        background-color: #ffffff;
        width: 100%;
        .headerlogo {
            width: 0.8rem;
            height: 0.8rem;
            position: absolute;
            left: 0.2rem;
            top: 0.23rem;
            border: 2px solid #fa9784;
            border-radius: 100%;
        }
        .username {
            font-size: 0.26rem;
            color: #333333;
            position: absolute;
            left: 1.1rem;
            top: 0.38rem;
        }
        .userId {
            position: absolute;
            left: 1.1rem;
            font-size: 0.2rem;
            color: #999999;
            top: 0.71rem;
        }
        .help {
            position: absolute;
            right: 0.2rem;
            top: 0.37rem;
            img {
                width: 0.34rem;
                float: left;
            }
            span {
                display: block;
                font-size: 0.18rem;
                color: #333333;
            }
        }
    }
    .getBalance{
         position: absolute;
        width: 100%;
        height: 0.88rem;
        top: 1.25rem;
            border-bottom: 0.1rem solid #f7f7f7;
            .balanceTitle{
                float: left;
                margin-left:0.2rem ;
                width: 100%;
                height: 0.3rem;
                color: #666666;
                margin-top: 0.14rem;
                line-height: 0.3rem;
            }
            .balanceValue{
                width: 100%;
                line-height: 0.3rem;
                height: 0.3rem;
                float: left;
                font-weight: bold;
                margin-left: 0.2rem;
                color: #404040;
            }
            .getbtn{
                position: absolute;
                right: 0.2rem;
                height: 0.36rem;
                width: 0.96rem;
                font-size: 0.24rem;
                color: #FFFFFF;
                background-color: #75d1f8;
                border-radius: 0.05rem;
                line-height: 0.36rem;
                margin-top: 0.21rem;
                text-align: center;
            }
    }
    .nav {
        position: absolute;
        width: 100%;
        top: 2.23rem;
        li {
            border-bottom: 0.1rem solid #f7f7f7;
            padding-left:0.2rem ;
            line-height: 0.6rem;
            text-align: left;
        }
    }
    .contextBox {
        position: absolute;
        width: 100%;
        top: 6.38rem;
        .ly_wap_help{
            img{
                width: 80%;
                margin-left: 10%;
            }
        }
        .ly_wap_news {
            li {
                background-color: #fff;
                border-bottom: 0.1rem solid #f7f7f7;
                .news_img_one {
                    padding: 0.2rem 0;
                    margin: 0 0.2rem;
                    background-color: #ffffff;
                    .title {
                        font-size: 0.28rem;
                        color: #000000;
                        padding-right: 0.2rem;
                    }
                    .authMess {
                        color: #dfdfdf;
                    }
                    .createTime {
                        color: #999999;
                        font-size: .2rem;
                        text-align: right;
                        padding-right: 0.2rem;
                    }
                    .username {
                        color: #878787;
                    }
                    .type {
                        color: #ffffff;
                        font-size: 0.22rem;
                        height: 0.3rem;
                        line-height: 0.3rem;
                    }
                    .img_left {
                        width: 1.84rem;
                        height: 1.24rem;
                        margin: 0 0.05rem;
                        margin-right: 0.26rem;
                        float: left;
                    }
                    .title {
                        height: 0.8rem;
                        display: -webkit-box;
                        -webkit-box-orient: vertical;
                        -webkit-line-clamp: 2;
                        overflow: hidden;
                    }
                }
            }
        }
        .ly_wap_package {
            margin-bottom: 0.88rem;
            li {
                float: left;
                background-color: #ffffff;
                img {
                    float: left;
                    width: 0.96rem;
                    height: 0.96rem;
                    border-radius: 0.2rem;
                    margin: 0.2rem 0.06rem 0.2rem 0.2rem;
                }
                .name {
                    font-size: 0.24rem;
                    margin-top: 0.28rem;
                    color: #000000;
                    float: left;
                    height: 0.24rem;
                }
                .progress {
                    float: left;
                    width: 3rem;
                    height: 0.34rem;
                    .box {
                        margin-top: 0.14rem;
                        float: left;
                        height: 0.06rem;
                        width: 1.9rem;
                        border-radius: 10%;
                        overflow: hidden;
                        background-color: #ffe8e3;
                        .currentPro {
                            height: 0.06rem;
                            width: 1.9rem;
                            border-radius: 10%;
                            background-color: #fa8d78;
                            -webkit-transform: translateX(-50%);
                        }
                    }
                    .tip {
                        line-height: 0.34rem;
                        font-size: 0.16rem;
                        color: #666666;
                        float: left;
                        line-height: 0.34rem;
                        margin-left: 0.1rem;
                    }
                }
                .outTime {
                    font-size: 0.16rem;
                    color: #666666;
                    float: left;
                    height: 0.16rem;
                }
                .state {
                    width: 0.75rem;
                    height: 0.36rem;
                    border-radius: 0.3rem;
                    position: absolute;
                    right: 0.2rem;
                    margin-top: 0.5rem;
                    text-align: center;
                    line-height: 0.36rem;
                }
                .state0 {
                    color: #ffffff;
                    background-color: #facb78;
                }
                .state1 {
                    color: #666666;
                    background-color: #eeeeee;
                }
            }
        }
    }
}
.packageTest,.platTest{
    margin: 0 0.2rem;
}

